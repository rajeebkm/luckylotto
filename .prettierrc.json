{
    "trailingComma": "none",
    "useTabs": false,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 4,
    "printWidth": 120,
    "plugins": ["prettier-plugin-solidity"],
    "overrides": [
        {
            "files": "*.sol",
            "options": {
                "parser": "solidity-parse",
                "printWidth": 120,
                "tabWidth": 4,
                "useTabs": false,
                "singleQuote": false,
                "semi": true
            }
        }
    ]
}
